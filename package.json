{
	"name": "quick-console",
	"displayName": "quick-console",
	"description": "insert and remove console statement quickly",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:quick-console.insertConsoleLog",
		"onCommand:quick-console.removeConsoleLog"
	],
	"contributes": {
		"commands": [
			{
				"command": "quick-console.insertConsoleLog",
				"title": "Insert ConsoleLog statement with selection"
			},
			{
				"command": "quick-console.removeConsoleLog",
				"title": "Remove ConsoleLog statements in current document"
			}
		],
		"keybindings": [
			{
				"command": "quick-console.insertConsoleLog",
				"key": "shift+ctrl+l",
				"mac": "shift+cmd+l",
				"when": "editorTextFocus"
			},
			{
				"command": "quick-console.removeConsoleLog",
				"key": "shift+ctrl+d",
				"mac": "shift+cmd+d"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}